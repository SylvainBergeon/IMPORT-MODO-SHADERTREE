#usda 1.0

def Scope "shadertree"
{
    def Material "Body_Material"
    {
        token outputs:mtlx:displacement.connect = </shadertree/Body_Material/DisplacementMap_Image_displacement.outputs:out>
        token outputs:mtlx:surface.connect = </shadertree/Body_Material/Material.outputs:surface>

        def Shader "Material"
        {
            uniform token info:id = "ND_standard_surface_surfaceshader"
            float inputs:base = 0.85
            color3f inputs:base_color = (0.19643234, 0.43670887, 0.22239327)
            float inputs:coat = 0
            float inputs:coat_roughness = 0
            float inputs:emission = 0
            color3f inputs:emission_color = (1, 1, 1)
            float inputs:opacity = 1
            float inputs:sheen = 0.195
            float inputs:specular = 1
            float inputs:specular_anisotropy = 0
            color3f inputs:specular_color = (1, 1, 1)
            float inputs:specular_IOR = 1.65
            float inputs:specular_roughness = 0.225
            float inputs:subsurface = 0
            color3f inputs:subsurface_color = (0, 0, 1.0000001)
            float inputs:subsurface_radius = 0.0635
            float inputs:subsurface_scale = 0.09
            float inputs:transmission = 0
            color3f inputs:transmission_color = (0.1663028, 0.306962, 0.08247385)
            float inputs:transmission_depth = 0.212
            float inputs:transmission_dispersion = 0
            float inputs:transmission_extra_roughness = 0.21
            float inputs:transmission_scatter = 1
            token outputs:surface
        }

        def Shader "DisplacementMap_texture_reader"
        {
            uniform token info:id = "ND_texcoord_vector2"
            int inputs:index = 0
            float2 outputs:out
            texCoord2f outputs:result
        }

        def Shader "DisplacementMap_imageFile"
        {
            uniform token info:id = "ND_UsdUVTexture_23"
            asset inputs:file = @/Volumes/SSD-4TO-PROJETS/HOUDINI_PROJECTS/IMPORT MODO SHADERTREE/Houdini_Export_Testfiles/Alien/DisplacementMap.tif@
            float2 inputs:st.connect = </shadertree/Body_Material/DisplacementMap_texture_reader.outputs:out>
            string inputs:wrapS = "periodic"
            string inputs:wrapT = "periodic"
            color3f outputs:rgb
        }

        def Shader "DisplacementMap_valueRange"
        {
            uniform token info:id = "ND_remap_color3"
            color3f inputs:in.connect = </shadertree/Body_Material/DisplacementMap_imageFile.outputs:rgb>
            color3f inputs:outhigh = (1, 1, 1)
            color3f inputs:outlow = (-1, -1, -1)
            color3f outputs:out
        }

        def Shader "DisplacementMap_Image_displacement"
        {
            uniform token info:id = "ND_displacement_float"
            float inputs:displacement.connect = </shadertree/Body_Material/DisplacementMap_valueRange.outputs:out>
            float inputs:scale = 0.07
            float outputs:out
        }
    }

    def Material "Nails_Material"
    {
        token outputs:mtlx:surface.connect = </shadertree/Nails_Material/Material_2.outputs:surface>

        def Shader "Material_2"
        {
            uniform token info:id = "ND_standard_surface_surfaceshader"
            float inputs:base = 0.8
            color3f inputs:base_color = (1, 1, 1)
            float inputs:coat = 0
            float inputs:coat_roughness = 0
            float inputs:emission = 0
            color3f inputs:emission_color = (1, 1, 1)
            float inputs:opacity = 1
            float inputs:sheen = 0
            float inputs:specular = 1
            float inputs:specular_anisotropy = 0
            color3f inputs:specular_color = (1, 1, 1)
            float inputs:specular_IOR = 1
            float inputs:specular_roughness = 0.4
            float inputs:subsurface = 0
            color3f inputs:subsurface_color = (0.8, 0.8, 0.8)
            float inputs:subsurface_radius = 1
            float inputs:subsurface_scale = 0.005
            float inputs:transmission = 0
            color3f inputs:transmission_color = (1, 1, 1)
            float inputs:transmission_depth = 0
            float inputs:transmission_dispersion = 0
            float inputs:transmission_extra_roughness = 0
            float inputs:transmission_scatter = 1
            token outputs:surface
        }
    }

    def Material "Eyes_Material"
    {
        token outputs:mtlx:surface.connect = </shadertree/Eyes_Material/Material_3.outputs:surface>

        def Shader "Material_3"
        {
            uniform token info:id = "ND_standard_surface_surfaceshader"
            float inputs:base = 0.8
            color3f inputs:base_color = (1, 1, 1)
            float inputs:coat = 0
            float inputs:coat_roughness = 0
            float inputs:emission = 0
            color3f inputs:emission_color = (1, 1, 1)
            float inputs:opacity = 1
            float inputs:sheen = 0
            float inputs:specular = 1
            float inputs:specular_anisotropy = 0
            color3f inputs:specular_color = (1, 1, 1)
            float inputs:specular_IOR = 1
            float inputs:specular_roughness = 0.4
            float inputs:subsurface = 0
            color3f inputs:subsurface_color = (0.8, 0.8, 0.8)
            float inputs:subsurface_radius = 1
            float inputs:subsurface_scale = 0.005
            float inputs:transmission = 0
            color3f inputs:transmission_color = (1, 1, 1)
            float inputs:transmission_depth = 0
            float inputs:transmission_dispersion = 0
            float inputs:transmission_extra_roughness = 0
            float inputs:transmission_scatter = 1
            token outputs:surface
        }
    }
}

